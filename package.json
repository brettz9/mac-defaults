{
  "name": "mac-defaults",
  "version": "2.0.0",
  "description": "access the macOS/OS X/MacOS X user defaults system",
  "author": "Tarran Jones <tarrandavidjones@gmail.com> (http://brettz9.com)",
  "contributors": [
    "Brett Zamir <brettz9@yahoo.com> (http://brett-zamir.me)"
  ],
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "exports": {
    "import": "./index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "jsdoc": "jsdoc --private --pedantic --configure jsdoc.conf.json . README.md package.json",
    "eslint": "eslint .",
    "lint": "npm run eslint --",
    "rollup": "rollup -c",
    "attw": "attw --pack",
    "test": "c8 ava --concurrency=4 --timeout=60s",
    "test-main": "ava test/test.js --concurrency=4 --timeout=60s",
    "test-parser": "ava test/parser.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brettz9/mac-defaults.git"
  },
  "keywords": [
    "macos",
    "mac",
    "osx",
    "mas",
    "os-x",
    "user",
    "defaults",
    "system",
    "read",
    "read-type",
    "write",
    "rename",
    "'delete'",
    "domains",
    "find",
    "help",
    "plist",
    "domain",
    "NSGlobalDomain"
  ],
  "engines": {
    "node": ">= 8.0.0"
  },
  "bugs": {
    "url": "https://github.com/brettz9/mac-defaults/issues"
  },
  "homepage": "https://github.com/brettz9/mac-defaults#readme",
  "dependencies": {
    "get-stream": "^6.0.1"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "ava": "^6.4.1",
    "c8": "^10.1.3",
    "eslint": "^7.24.0",
    "esm": "^3.2.25",
    "jsdoc": "^3.6.6",
    "rollup": "^4.53.3"
  }
}
